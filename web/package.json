{
  "name": "paired-ratings-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "bun@1.3.3",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "proto": "PATH=./node_modules/.bin:$PATH protoc -I=../proto --ts_proto_out=src/gen --ts_proto_opt=esModuleInterop=true,forceLong=number,useOptionals=none,snakeToCamel=false,outputServices=none,onlyTypes=true ../proto/paired_ratings.proto",
    "proto:watch": "bun run proto -- --watch",
    "format": "bunx prettier --write .",
    "format:check": "bunx prettier --check ."
  },
  "dependencies": {
    "@radix-ui/react-accordion": "1.2.12",
    "@radix-ui/react-alert-dialog": "1.1.15",
    "@radix-ui/react-checkbox": "1.3.3",
    "@radix-ui/react-collapsible": "1.1.12",
    "@radix-ui/react-dialog": "1.1.15",
    "@radix-ui/react-dropdown-menu": "2.1.16",
    "@radix-ui/react-hover-card": "1.1.15",
    "@radix-ui/react-popover": "1.1.15",
    "@radix-ui/react-scroll-area": "1.2.10",
    "@radix-ui/react-select": "2.2.6",
    "@radix-ui/react-separator": "1.1.8",
    "@radix-ui/react-slot": "1.2.4",
    "@radix-ui/react-tabs": "1.1.13",
    "@radix-ui/react-tooltip": "1.2.8",
    "@tailwindcss/vite": "4.1.18",
    "@tanstack/react-query": "5.90.12",
    "@tanstack/react-query-devtools": "5.90.12",
    "@tanstack/react-router": "1.142.8",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "cmdk": "1.1.1",
    "date-fns": "4.1.0",
    "lucide-react": "0.562.0",
    "react": "19.2.0",
    "react-day-picker": "9.13.0",
    "react-dom": "19.2.0",
    "sonner": "2.0.7",
    "tailwind-merge": "3.4.0",
    "tailwindcss": "4.1.18"
  },
  "devDependencies": {
    "@eslint/js": "9.39.1",
    "@types/node": "25.0.3",
    "@types/react": "19.2.5",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.1",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "9.39.1",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.4.24",
    "globals": "16.5.0",
    "prettier": "3.7.4",
    "prettier-plugin-tailwindcss": "0.7.2",
    "ts-proto": "2.10.0",
    "tw-animate-css": "1.4.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.50.1",
    "vite": "npm:rolldown-vite@7.3.0",
    "vite-plugin-pwa": "1.2.0"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.3.0"
  }
